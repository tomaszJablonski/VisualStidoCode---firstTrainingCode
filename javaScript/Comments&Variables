/*****Variables*******/

//pierwszy sposób
let number1 = 10;
let number2 = 10;

let result =  number1 + number2
console.log(result);

let message = 'Hello';
message = 'Welcome'; //ta zostanie zastąpiąne przez pierwszą z góry

//drugi sposób
let x,y;
x = 10;
y = 20;

//trzeci sposób
let a = 10, b=10; 

//można też tak
let name;
let Name; 
//to są dwie różne zmienne

//Użuwac camelCase
